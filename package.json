{
  "name": "@vaultinum/feature-map",
  "version": "1.0.1",
  "description": "Describe your product features and link them to your tests",
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "tsc",
    "featuremap": "node cli.js --config ./test/featuremap.config.json",
    "test": "jest --json --outputFile=./test/jest-results.json",
    "posttest": "npm run featuremap",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "8.3.0",
    "indent-string": "4.0.0",
    "lodash": "4.17.21",
    "typescript": "4.5.2",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.177",
    "@types/node": "16.11.7",
    "@types/yaml": "1.9.7",
    "@vaultinum/prettier-config": "1.0.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "prettier": "2.5.0",
    "pretty-quick": "3.1.2",
    "standard-version": "9.3.1",
    "ts-jest": "27.0.7"
  },
  "bin": {
    "featuremap": "./cli.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": "@vaultinum/prettier-config"
}
